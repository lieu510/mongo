<h1>Mongo Scraper</h1>

<h2>Reddit Edition</h2>

<button id="scrape">Scrape New Articles</button>
<a href="/saved">View Saved Articles</a>

<ul>
  {{#each articles}}
  	<li>
      <p><span class="label">Title:</span> {{this.title}}</p>
      <p><span class="label">Description:</span> {{this.description}}</p>

      <button data-id="{{this.id}}" class="save">Save Article</button>
  	</li>
  {{/each}}
</ul>
